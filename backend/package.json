{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier \"**/*.{js,ts,json}\" --write",
    "format:check": "prettier \"**/*.{js,ts,json}\" --check"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.45",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "prettier": "3.2.5"
  }
}
